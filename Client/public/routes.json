{
  "/api/cars": {
    "GET": {
      "action": "db.cars"
    },
    "POST": {
      "action": "db.cars.push(req.body)",
      "path": "/api/cars"
    }
  },
  "/api/cars/:id": {
    "GET": {
      "action": "db.cars.find(data => data.id == :id)"
    },
    "PUT": {
      "action": "db.cars.find(data => data.id == :id).completed = true"
    },
    "DELETE": {
      "action": "db.cars = db.cars.filter(data => data.id != :id)"
    }
  }
}